{
  "name": "webpack-angular2-example",
  "version": "0.0.0",
  "scripts": {
    "typings": "typings",
    "dev": "webpack-dev-server --config ./config/webpack.dev.js --content-base ./src --inline --progress",
    "dev:hot": "npm run dev -- --hot",
    "dev:ip": "npm run dev -- --host 192.168.0.2 --port 8080",
    "dev:ip:hot": "npm run dev:ip -- --hot",
    "pretest": "rimraf ./coverage",
    "test": "karma start ./config/karma.conf.js --single-run",
    "test:watch": "karma start ./config/karma.conf.js",
    "build": "webpack --config ./config/webpack.prod.js --progress --display-modules --bail",
    "build:ghp": "npm run build -- -my-ghp",
    "server": "http-server ./dist",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor ./config/protractor.chrome.js",
    "e2e:ip": "npm run e2e -- --baseUrl http://192.168.0.2:8080/",
    "e2e:ip:live": "npm run e2e:ip -- --elementExplorer",
    "doc": "typedoc --options ./typedoc.json ./src",
    "start": "npm run dev",
    "postinstall": "typings install"
  },
  "devDependencies": {
    "angular2": "2.0.0-beta.15",
    "angular2-hmr": "^0.5.5",
    "autoprefixer": "^6.3.6",
    "core-js": "^2.2.2",
    "css-loader": "^0.23.1",
    "es6-shim": "^0.35.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.6.1",
    "html-webpack-plugin": "^2.16.0",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "ng2lint": "^0.0.10",
    "node-sass": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.8.2",
    "protractor": "^3.2.2",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.2",
    "rimraf": "^2.5.2",
    "rxjs": "5.0.0-beta.2",
    "sass-loader": "^3.2.0",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.7.2",
    "tslint": "^3.7.4",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.10",
    "typings": "^0.7.12",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "zone.js": "^0.6.11"
  },
  "engines": {
    "node": ">= 5.10.0"
  },
  "private": true
}
